using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace FizzBuzz
{
    class Program
    {
        static void Main(string[] args)
        {
            int tree = 3;
            int fife = 5;

            for (int i = 1; i < 101; i++)
            {

                if (i % tree == 0 && i % fife == 0)
                    Console.WriteLine("FizzBuzz"); 
             
                else if (i % tree == 0)
                    Console.WriteLine("Fizz");

                else if (i % fife == 0)
                    Console.WriteLine("Buzz");

                else
                    Console.WriteLine(i);

            }
            Console.ReadKey();
        }
    }
}
